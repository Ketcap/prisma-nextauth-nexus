{
  "name": "prisma-nextauth-nexus",
  "version": "0.1.0",
  "private": true,
  "main": "function.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:local": "docker-compose up -d",
    "db:studio": "prisma studio --experimental",
    "db:seed:dev": "dotenv -- ts-node -r tsconfig-paths/register prisma/seed/dev.ts",
    "db:migrate:save": "dotenv -- prisma migrate dev --create-only",
    "db:migrate:deploy": "dotenv -- prisma migrate deploy",
    "db:migrate:status": "dotenv -- prisma migrate status",
    "generate": "npm run generate:prisma && npm run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "NODE_ENV=development ts-node -r dotenv/config -r tsconfig-paths/register --transpile-only ./domains/schema.ts",
    "postinstall": "npm run generate:prisma"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^0.5.3",
    "@octokit/rest": "^18.7.2",
    "@prisma/client": "^2.28.0",
    "apollo-server-micro": "^2.25.2",
    "dotenv": "^10.0.0",
    "evergreen-ui": "^6.3.0",
    "firebase-admin": "^9.11.0",
    "firebase-functions": "^3.14.1",
    "graphql": "^15.5.1",
    "micro": "^9.3.4",
    "next": "11.0.1",
    "next-auth": "^3.27.3",
    "nexus": "^1.1.0",
    "nexus-plugin-prisma": "^0.35.0",
    "nexus-prisma": "^0.30.0",
    "prisma": "^2.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^16.4.7",
    "@types/react": "^17.0.15",
    "dotenv-cli": "^4.0.0",
    "eslint": "7.31.0",
    "eslint-config-next": "11.0.1",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
